  <div class="container about" >
    <h1>Favorites</h1>
    <% @favorites.each do |favorite| %>
    <% if favorite %>
  <div class= "container about favorite" >
        <ul>
             <p> <%= link_to favorite.twitter_handle, search_results_path(twitter_handle: favorite.twitter_handle), id: "favorite"  %>
                     <%= button_to "Remove", delete_search_path(favorite.twitter_handle), class:"btn btn-danger", id:"remove_button" %></p>
      </ul>
                     <% end %>


            <% end %>

    </div>
  </div>